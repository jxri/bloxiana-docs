{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "Bloxiana",
  "colors": {
    "primary": "#658a95",
    "light": "#95b6bd",
    "dark": "#658a95"
  },
  "favicon": "favicon.ico",
  "navigation": {
    "groups": [
      {
        "group": "Get Started",
        "pages": [
          "index",
          "quickstart"
        ]
      },
      {
        "group": "Features",
        "pages": [
          "features/sniping"
        ]
      },
      {
        "group": "Server Configuration",
        "pages": [
          "configuration/user-tracking"
        ]
      }
    ]
  },
  "logo": {
    "light": "https://i.imgur.com/KJR0HfD.png",
    "dark": "https://i.imgur.com/KJR0HfD.png"
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "navbar": {
    "links": [
      {
        "label": "Support Server",
        "href": "https://discord.gg/bloxiana"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Home",
      "href": "https://bloxiana.lol"
    }
  },
  "seo": {
    "metatags": {
      "theme-color": "#658a95",
      "og:site_name": "Bloxiana"
    },
    "indexing": "navigable"
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/bloxiana"
    }
  },
  "redirects": [
    {
      "source": "/commands",
      "destination": "https://bloxiana.lol/commands"
    },
    {
      "source": "/status",
      "destination": "https://bloxiana.lol/status"
    },
    {
      "source": "/invite",
      "destination": "https://bloxiana.lol/invite"
    },
    {
      "source": "/discord",
      "destination": "https://discord.gg/bloxiana"
    }
  ]
}